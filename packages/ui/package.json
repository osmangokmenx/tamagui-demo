{
  "name": "ui",
  "version": "0.0.0",
  "dependencies": {
    "@tamagui/animations-react-native": "^1.8.1",
    "@tamagui/font-inter": "^1.8.1",
    "@tamagui/lucide-icons": "^1.8.1",
    "@tamagui/react-native-media-driver": "^1.8.1",
    "@tamagui/shorthands": "^1.8.1",
    "@tamagui/theme-base": "^1.8.1",
    "expo-font": "^11.0.1",
    "react": "^18.2.0",
    "react-native-svg": "^13.8.0",
    "solito": "^3.0.0",
    "tamagui": "^1.8.1"
  },
  "devDependencies": {
    "@tamagui/build": "^1.8.1",
    "eslint": "^8.29.0",
    "prettier": "latest",
    "typescript": "^5.0.2"
  },
  "files": [
    "types",
    "dist"
  ],
  "main": "./src/index.ts",
  "module:jsx": "src",
  "private": true,
  "scripts": {
    "build": "tamagui-build",
    "format": "prettier --write \"**/*.{ts,tsx}\" --config ../../.prettierrc",
    "format:check": "prettier -c \"**/*.{ts,tsx}\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc -b",
    "watch": "tamagui-build --watch"
  },
  "sideEffects": [
    "*.css"
  ],
  "types": "./types/index"
}
